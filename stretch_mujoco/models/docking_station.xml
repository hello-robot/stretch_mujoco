<mujoco model="docking_station">
  <compiler angle="radian" assetdir="assets" />
  <default>
    <default class="visual_ds">
      <geom group="2" type="mesh" contype="0" conaffinity="0"/>
    </default>
    <default class="collision_ds">
      <geom group="3" type="mesh"/>
    </default>
  </default>
  <asset>
    <texture type="2d" name="docking_station_aruco_sticker" file="docking_station_aruco_sticker.png"/>

    <material name="Black" specular="0.5" shininess="0.25" rgba="0.000000 0.000000 0.000000 1.000000"/>
    <material name="Shiny" specular="0.5" shininess="0.5683440550000001" rgba="0.800000 0.800000 0.800000 1.000000"/>
    <material name="Aruco_Docking_Station_Sticker" texture="docking_station_aruco_sticker" specular="0.5" shininess="0.25"/>

    <mesh file="link_docking_base_0.obj"/>
    <mesh file="link_docking_base_1.obj"/>
    <mesh name="link_docking_base" file="link_docking_base.STL"/>
    <mesh file="link_docking_aruco.obj"/>
  </asset>
  <worldbody>
    <body name="link_docking_base" pos="-1.0 0 0">
      <freejoint/>
      <geom mesh="link_docking_base_0" material="Shiny" class="visual_ds"/>
      <geom mesh="link_docking_base_1" material="Black" class="visual_ds"/>
      <!-- <geom mesh="link_docking_base_0" class="collision_ds"/>
      <geom mesh="link_docking_base_1" class="collision_ds"/> -->
      <geom class="collision_ds" mesh="link_docking_base" mass="0.386736"/>
      <body name="link_docking_aruco" pos="-0.326626 0 0.20514" quat="0.602908 -0.369462 0.369462 -0.602908">
        <geom material="Aruco_Docking_Station_Sticker" mesh="link_docking_aruco" class="visual_ds"/>
        <geom mesh="link_docking_aruco" class="collision_ds"/>
      </body>
    </body>
  </worldbody>
</mujoco>
